{
    "name": "mdlb/chokidar-wrapper",
    "description": "A wrapper of the node package chokidar using Laravel Events",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Mdlb\\Chokidar\\": "src/"
        }
    },
    "support": {
        "issues": "https://github.com/mdlb/chokidar-wrapper/issues",
        "source": "https://github.com/mdlb/chokidar-wrapper"
    },
    "authors": [
        {
            "name": "TomatoPana",
            "email": "22053906+TomatoPana@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^8.1.0",
        "laravel/framework": "^10.0|^11.0",
        "symfony/console": "^6.0|^7.0"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.10.15",
        "phpunit/phpunit": "^10.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mdlb\\Chokidar\\ChokidarServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}